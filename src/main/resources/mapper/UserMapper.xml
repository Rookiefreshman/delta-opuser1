<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="cn.chinatelecom.bigdata.delta.opuser.mapper.UserMapper">
    <select id="findByUserName" resultType="cn.chinatelecom.bigdata.delta.opuser.entity.User">
        SELECT User.*
        FROM User
        where User.`username`=#{username}
    </select>


</mapper>
